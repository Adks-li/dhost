{
  "name": "dhost",
  "version": "0.3.3",
  "type": "module",
  "description": "Never-caching development Node webserver",
  "homepage": "https://github.com/samthor/dhost",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./bundle.cjs"
    },
    "./rware/*.js": {
      "import": "./rware/*.js"
    }
  },
  "types": "types/index.d.ts",
  "repository": "git@github.com:samthor/dhost.git",
  "author": "Sam Thorogood <sam.thorogood@gmail.com>",
  "license": "Apache-2",
  "dependencies": {
    "async-transforms": "^1.0.7",
    "bytes": "^3.1.0",
    "colorette": "^1.2.1",
    "esm-resolve": "^1.0.4",
    "gumnut": "^0.3.1",
    "he": "^1.2.0",
    "mime": "^2.4.0",
    "mri": "^1.1.4"
  },
  "bin": "./bin/index.js",
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/he": "^1.1.1",
    "@types/mime": "^2.0.3",
    "@types/mri": "^1.1.0",
    "@types/node": "^14.14.24",
    "rollup": "^2.47.0"
  },
  "scripts": {
    "prepublishOnly": "rollup --format=cjs --file=bundle.cjs -- index.js "
  }
}
